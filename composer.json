{
    "name": "nexendrie/rss",
    "description": "Rss channel generator",
    "keywords": [
        "rss",
        "generator",
        "syndication",
        "feed"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "docs": "https://nexendrie.gitlab.io/rss"
    },
    "require": {
        "php": ">=8.3.0",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "psr/event-dispatcher": "^1.0",
        "nette/utils": "^3.2.7",
        "nexendrie/utils": "^1.10",
        "symfony/options-resolver": "^6.0 || ^7.0",
        "symfony/polyfill-php84": "^1.30"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nette/di": "^3.0.11",
        "nette/application": "^3.1.7",
        "nexendrie/site-generator": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Rss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\Rss\\": "tests/Nexendrie/Rss/"
        }
    },
    "suggest": {
      "nette/application": "To use RssResponse"
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
