{
    "name": "nexendrie/rss",
    "description": "Rss channel generator",
    "keywords": [
        "rss", "generator"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io/rss",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/rss/",
        "issues": "https://gitlab.com/nexendrie/rss/issues",
        "docs": "https://nexendrie.gitlab.io/rss"
    },
    "require": {
        "php": ">=5.6.0",
        "ext-simplexml": "*",
        "nette/utils": "^2.4"
    },
    "require-dev": {
        "nette/tester": "2.0.x-dev as v1.7",
        "mrtnzlml/testbench": "^2.3",
        "tracy/tracy": "^2.4",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "squizlabs/php_codesniffer": "^2.7",
        "nette/di": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Rss\\": "src/"
        },
        "classmap": ["src/exceptions.php"]
    },
    "suggest": {
      "nette/application": "To use RssResponse"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
