{
    "name": "nexendrie/rss",
    "description": "Rss channel generator",
    "keywords": [
        "rss", "generator"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://nexendrie.gitlab.io",
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/nexendrie/rss/",
        "issues": "https://gitlab.com/nexendrie/rss/issues",
        "docs": "https://nexendrie.gitlab.io/rss"
    },
    "require": {
        "php": ">=7.4.0",
        "ext-simplexml": "*",
        "ext-dom": "*",
        "nette/utils": "^3.2.2",
        "nexendrie/utils": "^1.8",
        "symfony/options-resolver": "^5.1",
        "symfony/polyfill-php80": "^1.22.1"
    },
    "require-dev": {
        "nexendrie/code-quality": "dev-master",
        "nette/di": "^3.0.8",
        "nette/application": "^3.1.2",
        "nexendrie/site-generator": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "Nexendrie\\Rss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexendrie\\Rss\\": "tests/Nexendrie/Rss/"
        }
    },
    "suggest": {
      "nette/application": "To use RssResponse"
    },
    "config": {
        "classmap-authoritative": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
